  "scripts": {
    "start": "node server.js",
    "lint": "eslint .",
    "fix-lint": "eslint --fix",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "dev": "nodemon server.js",
    "migrate": "node ./database/migration/index",
    "seed": "node ./database/seed/index",
    "db:seed": "npm run migrate & npm run seed",
