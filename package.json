    "test": "cross-env NODE_ENV=testing mocha --recursive --exit 'test'",
    "test:coverage": "cross-env NODE_ENV=testing nyc --reporter=html --reporter=text npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": " npm run format"
